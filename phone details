#include<stdio.h>
void main(){
	struct mobile{
		char model[15];
		int  id;
		int prize;
	};
	struct mobile a,b;
	printf("type the model of the android phone\n");
	scanf("%s",&a.model);
	printf("type the id of the android phone\n");
	scanf("%d",&a.id);
	printf("type the prize of the android phone\n");
	scanf("%d",&a.prize);
		printf("\n\ntype the model of the ios phone\n");
	scanf("%s",&b.model);
	printf("type the id of the ios phone\n");
    scanf("%d",&b.id);
	printf("type the prize of the ios phone\n");
	scanf("%d",&b.prize);
	printf("\nmodel name:%s\n model id:%d\n model prize:%d RS\n",a.model,a.id,a.prize);
		printf("\nmodel name:%s\n model id:%d\n model prize:%d RS",b.model,b.id,b.prize);
}

