	c=a+b;
	scanf("%d",c);
}


#include<stdio.h>
void main(){
	int a,b,c;
	scanf("%d",&a);
	scanf("%d",&b);
	scanf("%d",&c);
	if(a>b){
		if(a>c){
			printf("%d is greater",a);
		}
		else(c>b);{
			printf("%d is greater",c);
		}
	}
	else{
		printf("%d is greater",b);
	}
	
}
	
	


