}


#include<stdio.h>
void main(){
	int a;
	scanf("%d",&a);
	if(a%4==0){
		printf("%d is leap year",a);
	}
	else{
		printf("%d is not a leap year",a);
	}
}


#include<stdio.h>
void main(){
    int a,b;
	scanf("%d",&a);
	scanf("%d",&b);
	if(a>b){
		printf("%d is greater than %d ",a,b);
	}
	else if(b>a){
		printf("%d is grater than %d",b,a);
	}
	else{
		printf("%d and %d are equal ",a,b);
	}
	
	
	
}


#include<stdio.h>
void main(){
	int a;
	scanf("%d",&a);
	if(a>0){
		printf("%d is the positive number",a);
	}
	else if(a<0){
		printf("%d is a negative number",a);
	}
	else{
		printf("%d is equal to zero",a);
	}
	
}


#include<stdio.h>
void main(){
	int a;
	scanf("%d",&a);
	if(a%2==0){
		printf("%d is a even number",a);
	}
	else{
		printf("%d is a odd number",a);
	}
}
